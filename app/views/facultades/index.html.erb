<br><br><br><br>
<div class="w3-row-padding">
  <div class="w3-half">
    <div class="w3-card white">
      <div class="w3-container w3-red">
	<h1>Facultades</h1>
      </div>
      <ul class="w3-ul w3-border-top">
	<% @facultades.each do |facultad| %>
	<li>
	  <%= link_to facultad.nombre, facultad %>
	</li>
	<% end %>	
      </ul>
    </div>
  </div>
  <br>
  
  <div class="w3-half">
    <div class="w3-right">
      <form>
        <input class="w3-input" type="text" required>
        <br>
        <div class="w3-center">
          <button class="w3-btn w3-theme">Buscar</button>
        </div>
      </form>
    </div>
  </div>
  
</div>
